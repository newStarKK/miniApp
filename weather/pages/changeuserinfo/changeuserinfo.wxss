/* pages/changeuserinfo/changeuserinfo.wxss */ 
.listone{
  width:690rpx;
  height:90rpx;
  background:rgba(255,255,255,1);
  box-shadow:2rpx 3rpx 7rpx 0rpx rgba(117,117,117,0.2);
  border-radius:10rpx;
  padding: 0rpx 23rpx 0 38rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: space-between;
  margin: 30rpx auto 
}
.listone text:nth-child(1){
  display: block;
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
} 
.listone text:nth-child(2){
  display: block;
  font-size:32rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
} 
.listone view image:nth-child(1){
  width: 60rpx;
  height: 60rpx;
  margin-right: 17rpx;vertical-align: middle
}
.listone view image:nth-child(2){
  width: 14rpx;
  height: 25rpx;
  vertical-align: middle
}
.btn{
  width: 630rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #4A86FF;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  font-size: 30rpx;
  margin:276rpx auto 0
}
.listone input{
  width: 200rpx;
  height: 100%;
  text-align: right;
   font-size:32rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
}